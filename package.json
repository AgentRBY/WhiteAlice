{
  "name": "white_alice",
  "version": "1.0.0",
  "description": "White Alice: Multi-proposal Discord Bot",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec \"npm run build && npm run start\" ./dist/app.js --ignore dist -e ts",
    "start": "node ./dist/app.js",
    "lint": "eslint ./src --cache",
    "lint:fix": "eslint ./src --fix --cache",
    "prettier": "prettier ./src --write",
    "build": "swc ./src -d dist --config-file .swcrc",
    "check-types": "tsc --noEmit"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@discordjs/builders": "^0.15.0",
    "@notionhq/client": "^2.2.5",
    "discord-modals": "^1.3.7",
    "discord.js": "^13.16.0",
    "dotenv": "^16.3.1",
    "emoji-regex": "^10.2.1",
    "fast-fuzzy": "^1.12.0",
    "glob": "^10.2.7",
    "moment": "^2.29.4",
    "mongoose": "^7.3.0",
    "node-html-parser": "^6.1.5",
    "picocolors": "^1.0.0",
    "plural-ru": "^2.0.2",
    "request": "^2.88.2",
    "trace.moe.ts": "^1.3.10",
    "tslib": "^2.5.3"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.66",
    "@types/eslint": "^8.40.2",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/node": "^20.3.1",
    "@types/prettier": "^2.7.3",
    "@types/request": "^2.48.8",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "chokidar": "^3.5.3",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^47.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.3"
  },
  "packageManager": "pnpm@9.1.0",
  "engines": {
    "node": ">18"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  }
}
