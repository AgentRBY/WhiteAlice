{
  "name": "white_alice",
  "version": "1.0.0",
  "description": "White Alice: Multi-proposal Discord Bot",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently -p none \"npm run dev:compile\" \"npm run dev:watch -q\"",
    "dev:watch": "nodemon -w dist ./dist/app.js",
    "dev:compile": "swc ./src -d dist --config-file .swcrc -w ",
    "start": "node ./dist/app.js",
    "lint": "eslint ./src --cache",
    "lint:fix": "eslint ./src --fix --cache",
    "prettier": "prettier ./src --write",
    "build": "swc ./src -d dist --config-file .swcrc",
    "check-types": "tsc --noEmit"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@discordjs/builders": "^0.15.0",
    "@discordjs/opus": "^0.8.0",
    "@discordjs/voice": "^0.13.0",
    "@distube/soundcloud": "^1.2.2",
    "@distube/spotify": "^1.4.2",
    "@distube/ytdl-core": "latest",
    "@notionhq/client": "^2.2.1",
    "anidbjs": "^2.4.4",
    "anilist-node": "^1.13.0",
    "discord-modals": "^1.3.7",
    "discord.js": "^13.12.0",
    "distube": "3.3.4",
    "dotenv": "^16.0.3",
    "emoji-regex": "^10.2.1",
    "fast-fuzzy": "^1.11.2",
    "glob": "^8.0.3",
    "moment": "^2.29.4",
    "mongoose": "^6.6.5",
    "nhentai-api": "3.4.3",
    "node-html-parser": "^6.1.1",
    "picocolors": "^1.0.0",
    "request": "^2.88.2",
    "sagiri-fork": "^3.2.1",
    "trace.moe.ts": "^1.3.10",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.9",
    "@types/eslint": "^8.4.6",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/glob": "^8.0.0",
    "@types/node": "^18.11.0",
    "@types/prettier": "^2.7.1",
    "@types/request": "^2.48.8",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "chokidar": "^3.5.3",
    "concurrently": "^7.4.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^44.0.2",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  },
  "packageManager": "pnpm@7.27.0",
  "engines": {
    "node": ">17.4"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "pnpm": {
    "patchedDependencies": {
      "nhentai-api@3.4.3": "patches/nhentai-api@3.4.3.patch"
    }
  }
}
