{
  "name": "hlbot",
  "version": "1.0.0",
  "description": "HL Discord Bot",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently -p none \"yarn dev:compile\" \"yarn dev:watch -q\"",
    "dev:watch": "nodemon -w dist ./dist/app.js",
    "dev:compile": "swc ./src -d dist --config-file .swcrc -w ",
    "start": "pm2-runtime start ecosystem.config.js --env production",
    "lint": "eslint ./src --cache",
    "lint:fix": "eslint ./src --fix --cache",
    "prettier": "prettier ./src --write",
    "build": "swc ./src -d dist --config-file .swcrc",
    "check-types": "tsc --noEmit"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@discordjs/builders": "^0.15.0",
    "@discordjs/opus": "^0.7.0",
    "@discordjs/voice": "^0.10.0",
    "@distube/soundcloud": "^1.1.0",
    "@distube/spotify": "^1.2.1",
    "@distube/ytdl-core": "latest",
    "anidbjs": "^2.4.4",
    "anilist-node": "^1.12.1",
    "discord-modals": "^1.3.7",
    "discord.js": "^13.8.1",
    "distube": "3.3.4",
    "dotenv": "^16.0.1",
    "fast-fuzzy": "^1.11.2",
    "glob": "^8.0.3",
    "moment": "^2.29.3",
    "mongoose": "^6.4.2",
    "nhentai-api": "^3.4.3",
    "node-html-parser": "^5.3.3",
    "picocolors": "^1.0.0",
    "pm2": "^5.2.0",
    "request": "^2.88.2",
    "sagiri-fork": "^3.2.0",
    "trace.moe.ts": "^1.3.9",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.209",
    "@types/eslint": "^8.4.5",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/glob": "^7.2.0",
    "@types/node": "^18.0.1",
    "@types/prettier": "^2.6.3",
    "@types/request": "^2.48.8",
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "@typescript-eslint/parser": "^5.30.4",
    "chokidar": "^3.5.3",
    "concurrently": "^7.2.2",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^43.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.18",
    "prettier": "^2.7.1",
    "ts-node": "^10.8.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  },
  "packageManager": "yarn@3.2.1",
  "engines": {
    "node": "17.4"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  }
}
